{
  "name": "identity-platform-api",
  "version": "2.11.0",
  "description": "OpenID Connect provider for synapse projects",
  "main": "server.js",
  "scripts": {
    "start-dev": "npm run migrate-seed && nodemon -e hbs,js,json server.js",
    "start-migrate": "npm run migrate && node server.js",
    "start": "node server.js",
    "drop-tables": "./src/bin/drop-tables.js",
    "rebuild-db": "npm run drop-tables && knex migrate:latest && knex seed:run",
    "migrate-seed": "knex migrate:latest && knex seed:run",
    "generate-keys": "node src/bin/generate-keys.js",
    "migrate": "knex migrate:latest",
    "test": "NODE_ENV=test knex migrate:latest && lab -lv",
    "test:watch": "NODE_ENV=test knex migrate:latest && nodemon --exec lab -lv",
    "create-test-db": "node ./src/bin/create-test-db.js",
    "rebuild-db-for-tests": "NODE_ENV=test npm run create-test-db && NODE_ENV=test npm run drop-tables && NODE_ENV=test knex migrate:latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/synapsestudios/oidc-platform"
  },
  "author": "Synapse Studios, LLC (http://synapsestudios.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/synapsestudios/oidc-platform/issues"
  },
  "homepage": "https://github.com/synapsestudios/oidc-platform",
  "dependencies": {
    "@azure/storage-blob": "^12.1.1",
    "@sendgrid/mail": "^7.0.1",
    "@sentry/node": "^5.15.5",
    "@synapsestudios/winston-sentry": "^1.1.0",
    "aws-sdk": "^2.395.0",
    "bcrypt": "1.0.3",
    "bookshelf": "0.14.2",
    "boom": "^4.2.0",
    "btoa": "^1.1.2",
    "confidence": "^3.0.0",
    "date-fns": "^1.29.0",
    "electrolyte": "^0.4.0",
    "glue": "^4.1.0",
    "good": "^7.1.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.1",
    "good-winston": "^4.0.0",
    "handlebars": "4.1.0",
    "hapi": "^16.7.0",
    "hapi-auth-jwt2": "^7.2.4",
    "hoek": "4.2.1",
    "ioredis": "^2.5.0",
    "knex": "^0.15.2",
